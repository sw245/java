
public class C0721_05 {

	public static void main(String[] args) {
		char ch = 'A';
		String str = "AB";
//		char ch3 = ''	// 에러 (아무 값이 없을 수 없음)
		char ch4 = ' '; // 빈 공백 문자 > 가능
		String str2 = "";	// String은 가능
		
		// 자료형 크기 순 정렬
		// String > double > float > long > int > short, char > byte > boolean
		// 크기가 작은 형태의 자료를 더 큰 형태의 자료형으로 바꾸는 것은 항상 가능 - 자동 변환
		// 큰 형태 >> 작은 형태 - 강제 형 변환 (불가능할 수도 있음)
		
		System.out.println(str2);
		System.out.println(str2+7);
		System.out.println(str2+7+8);
	}

}
