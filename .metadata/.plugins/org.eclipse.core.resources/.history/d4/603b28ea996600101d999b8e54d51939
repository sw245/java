package ja0722;

public class C0722_01 {

	public static void main(String[] args) {
		boolean a = true;
		boolean a_2 = false;
		byte b = 1;
		short c = 1;
		char d = 'a'; // a:97, A:65, '0':48
		char d_2 = 65;
		char d_3 = 'a';
		char d_4 = ' ';
//		char d_5 = '';		// 에러
//		char d_6 = 'ab';	// 에러
//		char d_7 = "a";	// 에러
		int e = 1;
		long f = (long)1;
		long f_3 = 1L;
		long f_4 = 1l;
		long f_2 = 1;
		float g = 1.1F;
		float g_2 = 1.1f;
		double h = 1.1;
		double h_2 = 1.1d;
		double h_3 = 1.1D;
		String s = "";
		String s_2 = " ";
		String s_3 = "abcdef";
		String s_4 = "a";
		String str = new String("안녕하세요.");

		
		// 타입 변환
		String k = "1";		// String 타입
		int num = Integer.parseInt(k);
		String k_2 = "10.5";
		double num2 = Double.parseDouble(k_2);	// String -> 실수형 변환
		
		double m = 1.9;
		int num3 = (int)m;	// 1
		System.out.print(num3);
		
		
		
		
	}

}
