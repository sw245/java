package ja0722;

public class C0722_01 {

	public static void main(String[] args) {
		boolean a = true;
		boolean a_2 = false;
		byte b = 1;
		short c = 1;
		char d = 'a'; // a:97, A:65, '0':48
		char d_2 = 65;
		char d_3 = 'a';
		int e = 1;
		long f = (long)1;
		long f_3 = 1L;
		long f_4 = 1l;
		long f_2 = 1;
		float g = 1.1F;
		float g_2 = 1.1f;
		double h = 1.1;
		double h_2 = 1.1d;
		double h_3 = 1.1D;

	}

}
