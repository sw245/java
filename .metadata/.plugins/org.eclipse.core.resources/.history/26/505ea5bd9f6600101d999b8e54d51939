package ja0722;

public class C0722_03 {

	public static void main(String[] args) {
		char c1 = 'a';
		System.out.println(c1);

		char c2 = 97;
		System.out.println(c2);
		
//		char c3 = c1 + (char)1;
		char c3 = (char)(c1 + 1);
		System.out.println(c3);
		
		char c4 = ++c1;
		System.out.println(c4);
		
	}

}
