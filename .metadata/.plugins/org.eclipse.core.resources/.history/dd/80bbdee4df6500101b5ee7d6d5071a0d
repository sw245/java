
public class c0721_02 {
	
	// 함수, 메소드  >> function()
	// main()  >> 모든 프로그램은 main() 함수를 찾아서 실행
	public static void main(String[] args) {
		// 프로그램 구현 영역
		
		// boolean,char,byte,short,int,long,float,double / String
		
		int a = 10;
		int b = 20;
		int A = 30;		// 변수명 >> 대소문자 구분
//		int 1a = 20; 	// 숫자로 변수명 시작 불가
		int a1 = 20;
		// 특수문자는 _와 $ 가능
		int a_1 = 50;
		int $_a = 60;
	}

	// 바깥 부분의 코드 실행 불가?
	
	
//	def main2():
//		return ""
	
}
